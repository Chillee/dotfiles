URxvt*font: -*-terminesspowerline-medium-*-*-*-18-*-*-*-*-*-*-*
URxvt*boldFont: -*-terminesspowerline-bold-*-*-*-18-*-*-*-*-*-*-*
URxvt*italicFont: -*-terminesspowerline-medium-*-*-*-18-*-*-*-*-*-*-*
URxvt*bolditalicFont: -*-terminesspowerline-bold-*-*-*-18-*-*-*-*-*-*-*

!Xft.dpi:	96
Xft.rgba:	rgb
Xft.antialias: 	true
Xft.hinting: 	true
Xft.autohint: 	false
Xft.hintstyle: 	hintfull
Xft.lcdfilter: 	lcddefault
